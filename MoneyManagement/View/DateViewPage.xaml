<?xml version="1.0" encoding="UTF-8"?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="MoneyManagement.DateViewPage">
	
	<ContentPage Title="Daily Record" Icon="week.png">
		<ContentPage.Content>
			<StackLayout Orientation="Horizontal">
				<Label Text="Date"/>
				<Label Text="Subject"/>
				<Label Text="Amount"/>
			</StackLayout>
		</ContentPage.Content>
	</ContentPage>


	<ContentPage Title="Weekly Expense" Icon="month.png">
		<ContentPage.Content>
			<StackLayout Orientation="Horizontal">
				<Label Text="Week"/>
				<Label Text="Expense"/>
			</StackLayout>
		</ContentPage.Content>
	</ContentPage>

</TabbedPage>
